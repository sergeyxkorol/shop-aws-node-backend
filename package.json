{
  "name": "shop-aws-node-backend",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "deploy": "npx sls deploy",
    "serverless:offline": "sls offline",
    "openapi:generate": "serverless openapi generate -o openapi.yml -f yaml -a 3.0.3",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "buildFillDBScript": "esbuild ./ProductService/data/fillDBTables.js --bundle --outfile=./dist/fillDBTables.js --platform=node --target=node18.0",
    "fillDatabase": "set REGION=us-east-1 && set PRODUCTS=products-shop-aws-node-backend-dev && set STOCKS=stocks-shop-aws-node-backend-dev && node ./dist/fillDBTables.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.284.0",
    "@aws-sdk/lib-dynamodb": "^3.284.0",
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "joi": "^17.8.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.14.25",
    "esbuild": "^0.17.10",
    "jest": "^29.4.3",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.39.0",
    "serverless-offline": "^12.0.4",
    "serverless-openapi-documenter": "^0.0.47",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
